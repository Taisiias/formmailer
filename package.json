{
  "name": "formmailer",
  "version": "0.1.1",
  "description": "HTTP server that emails data submitted on static website forms.",
  "main": "index.js",
  "scripts": {
    "test": "tsc && tslint .",
    "build": "tsc",
    "live": "nodemon --exec ./node_modules/.bin/ts-node -- ./src/index.ts",
    "start": "tsc && node ./build/src/index.js",
    "test-smtp-server": "tsc && node ./build/src/run-smtp.js",
    "test-http": "http-server ./test -o"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Taisiias/formmailer.git"
  },
  "keywords": [
    "forms",
    "mail",
    "http",
    "post",
    "static"
  ],
  "author": "Taisiia Sobchenko",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Taisiias/formmailer/issues"
  },
  "homepage": "https://github.com/Taisiias/formmailer#readme",
  "dependencies": {
    "@types/he": "^0.5.29",
    "@types/mustache": "^0.8.29",
    "@types/node": "^7.0.39",
    "@types/node-static": "^0.7.0",
    "@types/nodemailer": "^1.3.33",
    "@types/request": "^2.0.3",
    "@types/sqlite3": "^3.1.0",
    "@types/winston": "^2.3.4",
    "@types/yargs": "^6.6.0",
    "email-validator": "^1.1.1",
    "he": "^1.1.1",
    "mustache": "^2.3.0",
    "node-static": "^0.7.9",
    "nodemailer": "^4.0.1",
    "querystring": "^0.2.0",
    "request": "^2.81.0",
    "sqlite3": "^3.1.9",
    "typescript": "^2.4.2",
    "url": "^0.11.0",
    "winston": "^2.3.1",
    "yargs": "^8.0.2"
  },
  "devDependencies": {
    "http-server": "^0.10.0",
    "nodemon": "^1.11.0",
    "smtp-server": "^3.0.1",
    "ts-node": "^3.3.0",
    "tslint": "^4.5.1"
  }
